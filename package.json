{
  "name": "myPrint",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "license": "UNLICENSED",
  "scripts": {
    "start": "node ace serve --watch",
    "build": "npm install @swc/core-linux-x64-gnu && node ace build && node public/bin/server.js",
    "dev": "node ace serve",
    "cron": "node ace scheduler:run",
    "test": "node ace test",
    "lint": "eslint .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "initDB": "node ace db:seed",
    "migrate": "node ace migration:run"
  },
  "imports": {
    "#controllers/*": "./app/controllers/*.js",
    "#exceptions/*": "./app/exceptions/*.js",
    "#models/*": "./app/models/*.js",
    "#interfaces/*": "./app/interfaces/*.js",
    "#mails/*": "./app/mails/*.js",
    "#services/*": "./app/services/*.js",
    "#listeners/*": "./app/listeners/*.js",
    "#events/*": "./app/events/*.js",
    "#middleware/*": "./app/middleware/*.js",
    "#validators/*": "./app/validators/*.js",
    "#providers/*": "./providers/*.js",
    "#policies/*": "./app/policies/*.js",
    "#abilities/*": "./app/abilities/*.js",
    "#database/*": "./database/*.js",
    "#start/*": "./start/*.js",
    "#tests/*": "./tests/*.js",
    "#config/*": "./config/*.js",
    "#repositories/*": "./app/repositories/*.js",
    "#repositoriesInterface/*": "./app/repositoriesInterface/*.js"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.8.1",
    "@adonisjs/eslint-config": "^2.0.0-beta.6",
    "@adonisjs/prettier-config": "^1.4.0",
    "@adonisjs/tsconfig": "^1.4.0",
    "@japa/api-client": "^2.0.3",
    "@japa/assert": "^3.0.0",
    "@japa/plugin-adonisjs": "^3.0.1",
    "@japa/runner": "^3.1.4",
    "@swc/core": "^1.7.22",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.5.1",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "hot-hook": "^0.3.0",
    "pino-pretty": "^11.2.2",
    "prettier": "^3.3.3",
    "ts-node-maintained": "^10.9.4",
    "typescript": "~5.5"
  },
  "dependencies": {
    "@adonisjs/auth": "^9.2.3",
    "@adonisjs/core": "^6.14.1",
    "@adonisjs/cors": "^2.2.1",
    "@adonisjs/lucid": "^21.3.0",
    "@types/uuid": "^10.0.0",
    "@vinejs/vine": "^2.1.0",
    "adonis-autoswagger": "^3.63.0",
    "adonisjs-scheduler": "^2.0.0",
    "cura-wasm": "^1.5.2",
    "cura-wasm-definitions": "^1.5.1",
    "luxon": "^3.5.0",
    "pg": "^8.13.0",
    "puppeteer": "^23.11.1",
    "reflect-metadata": "^0.2.2",
    "uuid": "^11.0.5"
  },
  "hotHook": {
    "boundaries": [
      "./app/controllers/**/*.ts",
      "./app/middleware/*.ts",
      "./app/models/**/*.ts",
      "./app/providers/**/*.ts",
      "./app/services/**/*.ts",
      "./app/repositories/**/*.ts",
      "./app/repositoriesInterface/**/*.ts"
    ]
  },
  "overrides": {
    "strtok3": "8.0.1"
  },
  "resolutions": {
    "strtok3": "8.0.1"
  },
  "pnpm": {
    "overrides": {
      "strtok3": "8.0.1"
    }
  }
}
